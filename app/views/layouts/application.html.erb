<!DOCTYPE html>
<html>
  <head>
    <title>AquariumApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "partials/nav" %>
    <div class="notice">
      <%= flash[:notice] %>
    </div>
    <div class="error">
      <%= flash[:error] %>
    </div>
    <%= yield %>
  </body>
</html>
